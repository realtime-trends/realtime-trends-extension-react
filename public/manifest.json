{
  "short_name": "#Realtime Trends",
  "name": "#실시간 검색어 (Realtime Trends)",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>","storage","alarms", "tabs"
  ],
  "description": "실시간 급상승 검색어를 제공합니다.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "2.0.0",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "content_scripts": [
    {
        "runt_at" : "document_start",
        "matches": [
          "https://www.naver.com/*"
        ],
        "js": [
            "/static/js/content.js"
        ],
        "css": [
            "/static/css/content.css"
        ]
    }
  ],
  "background": {
    "scripts": ["/static/js/background.js"],
    "persistent": false
  }
}